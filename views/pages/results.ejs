<h2>Here are your results for <%= query.city %>, <%= query.state %></h2>

<% results.forEach(function(result) { %>
  <div class="flex-middle">
    <p class="inline"><%= result.name %></p>
    <p class="inline"><%= result.lat %>, <%= result.long %> </p>
    <form action="/lookup" method="GET">
      <input type="hidden" name="lat" value="<%= result.lat %>">
      <input type="hidden" name="long" value="<%= result.long %>">
      <input type="submit" value="View Details">
    </form>
  </div>
<% }) %>